{
    "name": "nadar/aspsms",
    "type": "library",
    "description": "Simple to use sms sending class for the aspsms.com gateway.",
    "keywords": ["sms","php", "api", "aspsms", "gateway", "curl"],
    "homepage": "https://github.com/nadar/aspsms",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Basil Suter",
            "homepage": "https://github.com/nadar",
            "role": "Creator"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Aspsms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aspsms\\Test\\": "tests/"
        }
    }
}
