{
    "name": "nadar/aspsms",
    "type": "library",
    "description": "php class for sending sms over aspsms",
    "keywords": ["sms","php"],
    "homepage": "https://github.com/nadar/aspsms",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "nadar",
            "homepage": "https://github.com/nadar",
            "role": "Creator"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
